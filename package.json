{
  "name": "stereo-api",
  "type": "module",
  "version": "0.0.0",
  "description": "API backend for Stereo",
  "repository": "https://github.com/hoangvvo/stereo-api.git",
  "author": "Hoang Vo (https://hoangvvo.com/)",
  "license": "AGPL-3.0",
  "scripts": {
    "start": "node dist/main.js",
    "clean": "npx rimraf dist",
    "build:ts": "tsc",
    "build:copy": "npx cpy ./*/*.graphql --parents --cwd=src/graphql ../../dist/graphql",
    "build": "npm run codegen && npm run build:ts && npm run build:copy",
    "dev": "node -r dotenv/config --loader ts-node/esm  ./src/main.ts",
    "codegen": "graphql-codegen",
    "lint": "eslint ./ --ext json,ts,tsx --ignore-path .gitignore"
  },
  "dependencies": {
    "@benzene/core": "^0.7.1",
    "@benzene/extra": "^0.2.1",
    "@benzene/http": "^0.3.1",
    "@benzene/ws": "^0.6.2",
    "@graphql-tools/load-files": "^6.3.2",
    "@graphql-tools/merge": "^6.2.14",
    "@graphql-tools/schema": "^7.1.5",
    "@sentry/node": "^6.5.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dataloader": "^2.0.0",
    "fast-json-stringify": "^2.7.6",
    "googleapis": "^74.2.0",
    "graphql": "^15.5.0",
    "graphql-scalars": "^1.9.3",
    "ioredis": "^4.27.3",
    "jose": "^3.12.2",
    "mongodb": "^3.6.9",
    "nanoid": "^3.1.23",
    "next-connect": "^0.10.1",
    "slug": "^5.0.1",
    "undecim": "^0.0.2",
    "ws": "^7.4.6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.5",
    "@graphql-codegen/typescript": "^1.22.1",
    "@graphql-codegen/typescript-resolvers": "^1.19.2",
    "@swc/core": "^1.2.59",
    "@swc/helpers": "^0.2.12",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/ioredis": "^4.26.4",
    "@types/mongodb": "^3.6.17",
    "@types/node": "^15.6.2",
    "@types/pino": "^6.3.8",
    "@types/slug": "^5.0.0",
    "@types/spotify-api": "^0.0.9",
    "@types/ws": "^7.4.4",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "cpy-cli": "^3.1.1",
    "dotenv": "^10.0.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "engines": {
    "node": "16.x"
  },
  "private": true
}
